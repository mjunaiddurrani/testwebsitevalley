    $(function(){
        var hasBeenTrigged = false;
        $(window).scroll(function() {
            if($(window).width() <= 767){
                return false;
            }else if ($(this).scrollTop() >= 100 && !hasBeenTrigged) { // if scroll is greater/equal then 100 and hasBeenTrigged is set to false.
              // $('.form_div').addClass('active');
              //  $('.toggle').addClass('active');
                hasBeenTrigged = true;
            }
            
        });
