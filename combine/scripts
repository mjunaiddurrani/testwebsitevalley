				});

				return this;
			}
		}]);

		return Plugin;
	}();

	$.fn[pluginName] = function (options) {

		return this.each(function () {

			var pluginOptions = $(this).data('countdown-options') || options;

			if (!$.data(this, "plugin_" + pluginName)) {

				$.data(this, "plugin_" + pluginName, new Plugin(this, pluginOptions));
			}
		});
	};
