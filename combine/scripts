    function showThankMessageJob(id, msg)

    {

        complete = '<div class="Complete">';

        complete += '<div class="Tick">';

        complete += '<svg width="32px" height="25px" viewBox="0 0 32 25">';

        complete += '<g id="Page-1" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">';

        complete += '<g id="Artboard" transform="translate(-384.000000, -217.000000)" fill-rule="nonzero" fill="#FFFFFF">';

        complete += '<g id="Group" transform="translate(360.000000, 189.000000)">';

        complete += '<path d="M27.4142136,40.5857864 C26.633165,39.8047379 25.366835,39.8047379 24.5857864,40.5857864 C23.8047379,41.366835 23.8047379,42.633165 24.5857864,43.4142136 L34,52.8284271 L55.4142136,31.4142136 C56.1952621,30.633165 56.1952621,29.366835 55.4142136,28.5857864 C54.633165,27.8047379 53.366835,27.8047379 52.5857864,28.5857864 L34,47.1715729 L27.4142136,40.5857864 Z" id="Path-2"></path>';

        complete += '</g>';

        complete += '</g>';

        complete += '</g>';

        complete += '</svg>';

        complete += '</div>';

        complete += '<h4>Thank you</h4>';

        complete += '<span>'+msg+'</span>';

        complete += '</div>';

    

        $("#"+id).html(complete);

        $("#"+id+" .Complete .Tick").css({"opacity": "1", "transform": "scale(1)"});

        $("#"+id+" .Complete svg").css({"opacity": "1", "transform": "scale(1)"});                          

    }

    

    

