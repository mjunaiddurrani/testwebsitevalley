// Game page slider

$(document).ready(function() {
    $('.slider-top-s').slick({
    slidesToShow: 1,
    slidesToScroll: 1,
    autoplay:false, 
    autoplaySpeed: 5000,
    arrows: false,
    fade: false,
    asNavFor: '.slider-nav-top-s',
 });

 $('.slider-nav-top-s').slick({
    slidesToShow: 6,
    slidesToScroll: 1,
    asNavFor: '.slider-top-s',
    dots: false,
    arrows: false,
    focusOnSelect: true,
    vertical:true,
    verticalSwiping:true
 });

 //remove active class from all thumbnail slides
 $('.slider-nav-top-s .slick-slide').removeClass('slick-active');

 //set active class to first thumbnail slides
 $('.slider-nav-top-s .slick-slide').eq(0).addClass('slick-active');

 // On before slide change match active thumbnail to current slide
 $('.slider-top-s').on('beforeChange', function (event, slick, currentSlide, nextSlide) {
    var mySlideNumber = nextSlide;
    $('.slider-nav-top-s .slick-slide').removeClass('slick-active');
    $('.slider-nav-top-s .slick-slide').eq(mySlideNumber).addClass('slick-active');
});
 });

