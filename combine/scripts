    function setDownloadCookie(cname, cvalue, exdays) {

        var d = new Date();

        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));

        var expires = "expires="+d.toUTCString();

        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";

    }



    function getDownloadCookie(cname) {

        var name = cname + "=";

        var ca = document.cookie.split(';');

        for(var i = 0; i < ca.length; i++) {

            var c = ca[i];

            while (c.charAt(0) == ' ') {

                c = c.substring(1);

            }

            if (c.indexOf(name) == 0) {

                return c.substring(name.length, c.length);

            }

        }

        return "";

    }





    function setDownload(e){

          download = e.getAttribute("data-file");

        setDownloadCookie("ebook_download_link", download, 1);

        setDownloadCookie("ebook_downloaded", 0, 1);

    }




function imageOne() {
  var element = document.getElementById("image1");
  element.classList.add("image1");
  element.classList.remove("image2");
  element.classList.remove("image3");
  element.classList.remove("image4");
  element.classList.remove("image5");
    
}
function imageTwo() {
  var element = document.getElementById("image1");
  element.classList.remove("image1");
  element.classList.add("image2");
  element.classList.remove("image3");
  element.classList.remove("image4");
  element.classList.remove("image5");
    
}
